<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/catmonkey_bk5"
    tools:context=".Catmonkey_page5">

    <TextView
        android:id="@+id/catmonkey_text5"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:text="@string/catmonkey_p5_kor"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.855" />

    <Button
        android:id="@+id/next_btn"
        android:layout_width="70dp"
        android:layout_height="66dp"
        android:background="@drawable/ic_nextpage"
        android:text=""
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/replay_btn"
        android:layout_width="61dp"
        android:layout_height="64dp"
        android:background="@drawable/ic_replay"
        android:text=""
        app:layout_constraintEnd_toStartOf="@+id/next_btn"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/monkey_p5_btn"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:background="@drawable/monkey_ic"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.247"
        app:layout_constraintStart_toEndOf="@+id/cat_p5_btn"
        app:layout_constraintTop_toBottomOf="@+id/catmonkey_text5"
        app:layout_constraintVertical_bias="0.6" />

    <Button
        android:id="@+id/cat_p5_btn"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:layout_marginStart="116dp"
        android:background="@drawable/cat_ic"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/catmonkey_text5"
        app:layout_constraintVertical_bias="0.589" />

    <Button
        android:id="@+id/eng_chg_btn"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_eng_chg"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/back_btn"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/back_btn"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_backpage"
        android:text=""
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>