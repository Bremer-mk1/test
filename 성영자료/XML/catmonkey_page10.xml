<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/moral"
    tools:context=".Catmonkey_page10">

    <TextView
        android:id="@+id/catmonkey_text10"
        android:layout_width="match_parent"
        android:layout_height="158dp"
        android:text="@string/catmonkey_p10_kor"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.539" />

    <Button
        android:id="@+id/next_btn"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_nextpage"
        android:text=""
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/replay_btn"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_replay"
        android:text=""
        app:layout_constraintEnd_toStartOf="@+id/next_btn"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/eng_chg_btn"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_eng_chg"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/back_btn"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/back_btn"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_backpage"
        android:text=""
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>